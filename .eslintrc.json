{
"extends": [
    "plugin:prettier/recommended",
    "stylelint-config-prettier",
    "prettier"
    ],
"parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
"env": {
      "es6": true
  },
  "plugins": ["prettier", "stylelint"],
  "rules": {"prettier/prettier": [
    "error", {"endOfLine":"auto"}
  ]}
}